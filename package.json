{
    "name": "@cojam/duxpanel",
    "author": "Ken Schenke",
    "version": "1.1.4",
    "description": "React JS Draggable Panels, Dialogs, and Modals",
    "homepage": "https://cojamru.github.io/duxpanel/",
    "license": "MIT",
    "main": "index.js",
    "engines": {
        "node": ">=14.14.0",
        "npm": ">=7.0.2"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/cojamru/duxpanel"
    },
    "keywords": [
        "react",
        "reactjs",
        "panel",
        "dialog",
        "modal",
        "drag",
        "draggable",
        "duxpanel"
    ],
    "scripts": {
        "test": "jest",
        "build": "gulp",
        "testbed": "webpack serve --env=testbed",
        "website": "webpack serve --env=website",
        "website-dist": "webpack --env=website-dist",
        "website-deploy": "npm run website-dist && gh-pages -d ./website/dist",
        "prepare": "npm run build",
        "prettier": "prettier --write **/*.{html,json,js,jsx,ts,tsx,css,scss}",
        "lint-staged": "lint-staged"
    },
    "lint-staged": {
        "*.{html,json,js,jsx,ts,tsx,css,scss}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint-staged"
        }
    },
    "dependencies": {
        "lodash.throttle": "4.1.1",
        "prop-types": "15.7.2"
    },
    "devDependencies": {
        "@fortawesome/fontawesome": "1.1.8",
        "@fortawesome/fontawesome-common-types": "0.2.32",
        "@fortawesome/fontawesome-free-solid": "5.0.13",
        "@fortawesome/fontawesome-svg-core": "1.2.32",
        "@fortawesome/react-fontawesome": "0.1.11",
        "@types/lodash.throttle": "4.1.6",
        "@types/prop-types": "15.7.3",
        "@types/react": "16.9.53",
        "@types/react-dom": "16.9.8",
        "@types/react-syntax-highlighter": "13.5.0",
        "copy-webpack-plugin": "6.2.1",
        "css-loader": "5.0.0",
        "css-minimizer-webpack-plugin": "1.1.5",
        "gh-pages": "3.1.0",
        "gulp": "4.0.2",
        "gulp-clean": "0.4.0",
        "gulp-clean-css": "4.3.0",
        "gulp-pipe": "1.0.4",
        "gulp-sourcemaps": "2.6.5",
        "gulp-terser": "1.4.0",
        "gulp-typescript": "6.0.0-alpha.1",
        "husky": "4.3.0",
        "jest": "26.6.0",
        "lint-staged": "10.4.2",
        "mini-css-extract-plugin": "1.1.0",
        "prettier": "2.1.2",
        "raw-loader": "4.0.2",
        "react-syntax-highlighter": "15.2.1",
        "remove-files-webpack-plugin": "1.4.4",
        "style-loader": "2.0.0",
        "terser-webpack-plugin": "5.0.0",
        "ts-loader": "8.0.6",
        "typescript": "4.0.3",
        "webpack": "5.1.3",
        "webpack-cli": "4.1.0",
        "webpack-dev-server": "3.11.0"
    },
    "peerDependencies": {
        "react": "16.x",
        "react-dom": "16.x"
    }
}
